{
  "name": "combohtml",
  "description": "Combo your HTML files, make scripts(.js) and stylesheets(.css) inline.",
  "version": "1.1.2",
  "engines": {
    "node": ">=4"
  },
  "main": "dist/combohtml.js",
  "module": "dist/combohtml.esm.js",
  "bin": {
    "combohtml": "bin/combohtml.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mingelz/combohtml.git"
  },
  "author": {
    "name": "mingelz",
    "email": "mingelz@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "combo",
    "mutiple",
    "cli",
    "inline",
    "source",
    "inline-source",
    "inliner"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint --fix --report-unused-disable-directives src",
    "compile": "rollup -c",
    "pretest": "npm run lint",
    "precompile": "npm run clean -s",
    "prepublishOnly": "npm run compile"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.11.0",
    "csso": "^3.2.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "uglify-js": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-mingelz": "^0.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0"
  },
  "files": [
    "bin",
    "dist"
  ]
}
